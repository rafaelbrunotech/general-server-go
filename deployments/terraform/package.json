{
  "name": "app",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "get": "cdktf get",
    "build": "tsc src/**/*.ts",
    "synth": "cdktf synth",
    "deploy": "cdktf deploy",
    "deploy-all": "tsc src/**/*.ts && cdktf deploy '*'",
    "destroy": "tsc src/**/*.ts && cdktf destroy",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint": "eslint --fix",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "cdktf": "0.20.11",
    "constructs": "10.4.2",
    "dotenv": "16.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/js": "9.21.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-import-helpers": "2.0.1",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-perfectionist": "4.9.0",
    "eslint-plugin-security": "3.0.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prettier": "3.5.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1"
  }
}
